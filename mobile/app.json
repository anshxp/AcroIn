{
  "expo": {
    "name": "AcroIn",
    "slug": "acroin",
    "version": "1.0.0",
    "scheme": "acroin",
    "orientation": "portrait",
    "entryPoint": "./App.js",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-router"
    ]
  }
}
